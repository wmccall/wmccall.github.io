{
  "name": "website",
  "version": "0.1.0",
  "homepage": "https://wmccall.github.io/",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "cp -r build/* ../",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --verboase -env=jsdom",
    "test:cover": "npm test --coverage --watchAll",
    "eject": "react-scripts eject",
    "lint:js": "eslint src",
    "lint:css": "stylelint './src/**/{styled.js,Styled.jsx,*.scss}'",
    "lint:todo": "fixme './src/**/*.{js,jsx}'",
    "lint": "npm-run-all lint:todo lint:css lint:js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "npm-run-all lint:css:fix lint:js:fix",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
